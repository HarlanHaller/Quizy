{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "sets",
        "metadata"
    ],
    "properties": {
        "sets": {
            "type": "array",
            "default": [],
            "title": "The sets Schema",
            "items": {
                "type": "object",
                "default": {},
                "title": "A Schema",
                "required": [
                    "cards",
                    "metadata"
                ],
                "properties": {
                    "cards": {
                        "type": "object",
                        "default": {},
                        "title": "The cards Schema"
                    },
                    "metadata": {
                        "type": "object",
                        "default": {},
                        "title": "The metadata Schema",
                        "required": [
                            "name",
                            "studied"
                        ],
                        "properties": {
                            "name": {
                                "type": "string",
                                "default": "mySet",
                                "title": "The name of the set"
                            },
                            "studied": {
                                "type": "array",
                                "default": [],
                                "title": "the studied cards"
                            }
                        },
                        "examples": [
                            {
                                "name": "prototype"
                            }
                        ]
                    }
                },
                "examples": [
                    {
                        "cards": {
                            "term": "definition"
                        },
                        "metadata": {
                            "name": "prototype"
                        }
                    }
                ]
            },
            "examples": [
                [
                    {
                        "cards": {
                            "term": "definition"
                        },
                        "metadata": {
                            "name": "prototype"
                        }
                    }
                ]
            ]
        },
        "metadata": {
            "type": "object",
            "default": {},
            "title": "The metadata Schema",
            "items": {
                "type": "object",
                "default": {},
                "title": "A Schema",
                "required": [
                    "last-opened"
                ]
            },
            "properties": {
                "last-opened": {
                    "type": "string",
                    "default": "",
                    "title": "The last set opened"
                }
            },
            "examples": [
                {
                    "last-opened": "prototype"
                }
            ]
        }
    },
    "examples": [
        {
            "sets": [
                {
                    "cards": {
                        "term": "definition"
                    },
                    "metadata": {
                        "name": "prototype"
                    }
                }
            ],
            "metadata": {
                "last-opened": "prototype"
            }
        }
    ]
}